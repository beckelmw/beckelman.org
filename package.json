    "concurrently": "^7.0.0",
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.12",
    "got": "^12.0.1",
    "hasha": "^5.2.2",
    "miniflare": "^2.3.0",
    "tailwindcss": "^3.0.15",
    "tempura": "^0.4.0"
  },
  "dependencies": {
    "itty-router": "^2.4.10",
